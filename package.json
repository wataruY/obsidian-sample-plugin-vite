{
  "name": "obsidian-sample-plugin",
  "version": "1.0.0",
  "private": true,
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "keywords": [],
  "license": "0-BSD",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "fmt": "oxfmt",
    "check": "tsgo -noEmit -skipLibCheck",
    "build": "tsgo -noEmit -skipLibCheck && vite build -m production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "oxlint -c .oxlintrc.jsonc",
    "test": "vitest"
  },
  "dependencies": {
    "obsidian": "latest"
  },
  "devDependencies": {
    "@eslint/js": "9.30.1",
    "@types/node": "^16.11.6",
    "@typescript/native-preview": "^7.0.0-dev.20260102.1",
    "builtin-modules": "^5.0.0",
    "esbuild": "0.25.5",
    "eslint-plugin-obsidianmd": "0.1.9",
    "globals": "14.0.0",
    "jiti": "2.6.1",
    "oxfmt": "^0.21.0",
    "oxlint": "^1.36.0",
    "oxlint-tsgolint": "^0.10.1",
    "rolldown": "^1.0.0-beta.58",
    "rolldown-vite": "^7.3.0",
    "tslib": "2.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.35.1",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.3.0"
  }
}
