{
  "name": "obsidian-sample-plugin",
  "version": "1.0.0",
  "private": true,
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "keywords": [],
  "license": "0-BSD",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "fmt": "oxfmt",
    "check": "tsgo -noEmit -skipLibCheck",
    "build-css": "bunx @tailwindcss/cli -i ./src/input.css -o ./styles.css",
    "build": "tsgo -noEmit -skipLibCheck && vite build -m production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "oxlint -c .oxlintrc.jsonc",
    "test": "vitest"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.3.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@szhsin/react-autocomplete": "^1.2.0",
    "@types/bun": "^1.3.5",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.562.0",
    "obsidian": "latest",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "use-debounce": "^10.1.0"
